# Type casting = Processo de convers√£o de uma variavel para outro tipo de dado 
# str(), int(), float(), bool()

nome = "Gabriel"
idade = 19
homem = True

print(type(nome))
print(type(idade))
print(type(homem))

print("Convertendo para outras classes na mesma sequencia:")

print(type(bool(nome)))
print(type(int(idade)))
print(type(str(homem)))

print(nome)
print(idade)
print(homem)